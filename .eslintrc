{
    "env": {
      "browser": true,
      "jest": true,
      "es6": true
    },
    "plugins": ["import", "@typescript-eslint"],
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-eval": "error",
      "import/first": "error"
    },
    "root": true
  }